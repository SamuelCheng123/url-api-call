{
    "manifest_version": 2,
    "name": "自動發送Request",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "permissions": [
      "webRequest",
      "tabs",
      "<all_urls>"
    ],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }]
}
  