{
    "manifest_version": 2,
    "name": "自動發送Request",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": [
      "tabs",
      "*://*.tradezeroweb.co/*"
    ],
    "content_scripts": [{
      "matches": ["*://*.tradezeroweb.co/*"],
      "js": ["content.js"]
    }]
}
  